{
  "name": "scout-platform-v5",
  "version": "5.0.0",
  "description": "TBWA Scout Platform v5 - Complete ETL to AI Insights Dashboard with Next.js Frontend",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "api:dev": "nodemon --exec ts-node scout-platform-api-server.ts",
    "api:build": "tsc",
    "api:start": "node dist/scout-platform-api-server.js",
    "supabase:dev": "supabase start && supabase functions serve",
    "supabase:deploy": "./deploy.sh deploy",
    "health": "./deploy.sh health",
    "logs": "./deploy.sh logs",
    "stop": "./deploy.sh stop",
    "clean": "./deploy.sh clean",
    "db:reset": "supabase db reset --local",
    "db:push": "supabase db push",
    "db:types": "supabase gen types typescript --schema scout > types/database.types.ts",
    "functions:deploy": "supabase functions deploy --no-verify-jwt",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "patch:analyze": "ts-node scripts/patch-components.ts analyze",
    "patch:components": "ts-node scripts/patch-components.ts patch",
    "patch:validate": "ts-node scripts/patch-components.ts validate",
    "schema:validate": "npm run patch:validate"
  },
  "keywords": [
    "tbwa",
    "scout",
    "retail-intelligence",
    "etl",
    "wren-ai",
    "rag",
    "supabase",
    "qdrant",
    "analytics",
    "dashboard",
    "nextjs"
  ],
  "author": "TBWA Data Intelligence Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@nivo/core": "^0.99.0",
    "@nivo/sankey": "^0.99.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/postcss": "^4.1.11",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.535.0",
    "moment": "^2.29.4",
    "next": "^15.4.5",
    "node-cron": "^3.0.3",
    "openai": "^4.0.0",
    "papaparse": "^5.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "19.1.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.21",
    "bunyan": "^1.8.15",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
